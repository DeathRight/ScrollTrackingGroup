  "description": "React Component that tracks which section is scrolled to, for SPA or quasi-carousel functionality.",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
